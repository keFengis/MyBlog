<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.flydog.vo.Surfwell" table="surfwell" schema="web">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="surfwellcontent">
            <column name="surfwellcontent" sql-type="mediumtext" not-null="true"/>
        </property>
        <property name="surfwelldelstate">
            <column name="surfwelldelstate" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="surfwelltitle">
            <column name="surfwelltitle" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>