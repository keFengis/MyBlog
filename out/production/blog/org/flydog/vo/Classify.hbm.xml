<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.flydog.vo.Classify" table="classify" schema="web">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="classname">
            <column name="classname" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="classaddtime">
            <column name="classaddtime" sql-type="timestamp"/>
        </property>
        <property name="classcode">
            <column name="classcode" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="postsnum">
            <column name="postsnum" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>