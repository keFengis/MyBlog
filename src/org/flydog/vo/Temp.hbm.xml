<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.flydog.vo.Temp" table="temp" schema="web">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="postid">
            <column name="postid" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="classname">
            <column name="classname" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>